<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:TasksCompletedView="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <FrameLayout
        android:layout_width="56dp"
        android:layout_height="56dp"
        android:layout_centerInParent="true">

        <com.yw.library.weight.CompletedView
            android:id="@+id/tasks_view"
            android:layout_width="56dp"
            android:layout_height="56dp"
            android:layout_centerInParent="true"
            TasksCompletedView:circleColor="@color/white"
            TasksCompletedView:radius="10dp"
            TasksCompletedView:ringBgColor="@color/color_FF7201"
            TasksCompletedView:ringColor="@color/color_FFF35F"
            TasksCompletedView:strokeWidth="4dp" />
    </FrameLayout>
</RelativeLayout>