<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--折叠View的styleable-->
    <declare-styleable name="app">
        <attr name="defaultChildWidth" format="integer" />
        <attr name="defaultChildHeight" format="integer" />
        <attr name="defaultChildMarginLeft" format="integer" />
        <attr name="defaultChildMarginRight" format="integer" />
        <attr name="defaultChildMarginTop" format="integer" />
        <attr name="defaultChildMarginBottom" format="integer" />
        <attr name="directionMode" format="integer" />
    </declare-styleable>
    <declare-styleable name="blur">
        <attr name="blurAlphaColor" format="reference|color" />
        <attr name="blurAlpha" format="integer" />
        <attr name="blurWidth" format="dimension" />
        <attr name="blurHeight" format="dimension" />
    </declare-styleable>
</resources>